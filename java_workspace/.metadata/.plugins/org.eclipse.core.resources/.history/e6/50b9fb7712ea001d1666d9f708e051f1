	}

}

//car 클래스 생성

class Car{
	
	private String color; 
	private int door; 
	private boolean power; 
	private int speed; 

	public Car() {
		
	}
	
	public Car(String color) {
		this.color = color;
	}
	
	public Car(String color , int door) {
		this.color = color;
		this.door = door;
	}
	
	public void powerOn() {
		System.out.println("시동이 켜졌습니다.");
		power = true;
	}
	
	public void powerOff() {
		System.out.println("시동이 꺼졌습니다.");
		power = false;
	}
	
	public void speedUp() {
		speed = speed+10;
		System.out.println();
	}
	
	
	
}

