package Prouduct;

public class Product {
	
		//메뉴, 가격 구성
		private String menu;
		private int price;
		private int cnt;
		
		public Product() {}
		public Product(String menu, int price, int cnt) {
			this.menu = menu;
			this.price = price;
			this.cnt = cnt;
		}
		public String getMenu() {
			return menu;
		}
		public void setMenu(String menu) {
			this.menu = menu;
		}
		public int getPrice() {
			return price;
		}
		public void setPrice(int price) {
			this.price = price;
		}
		@Override
		public String toString() {
			return menu + "	:" + price;
		}
		public int getCnt() {
			return cnt;
		}
		public void setCnt(int cnt) {
			this.cnt = cnt;
		}
		
		
	

}
