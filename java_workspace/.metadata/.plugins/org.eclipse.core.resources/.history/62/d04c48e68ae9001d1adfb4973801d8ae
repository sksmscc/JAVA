package reNew01;

public class New01 {

	public static void main(String[] args) {
		// 홍길동 주민번호 881002-1234567
		// 출력형태 : 생년월일 : 881002 , 성별 : 남자
		
		String pin = "881002-1234567";
		System.out.println(pin);
		System.out.println("생년월일 : "+pin);
		
		String result = pin.substring(pin.lastIndexOf("-")+1);
		
	}

}
