//car 클래스 생성

class Car{
	
	private String color; 
	private int door; 
	private boolean power; 
	private int speed; 

	public Car(String color) {
		this.color = color;
	}
	
	public Car(String color , int door) {
		this.color = color;
		this.door = door;
	}
	
	
}

